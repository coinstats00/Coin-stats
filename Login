import requests

url = 'https://ethereum-sepolia.gateway.tatum.io/'
headers = {
  'accept': 'application/json',
  'content-type': 'application/json',
  'x-api-key': '<INSERT YOUR API KEY>'
}
body = {
  'jsonrpc': '2.0',
  'method': 'eth_blockNumber',
  'id': 1
}

response = requests.post(url, headers=headers, json=body)
print(response.text)
